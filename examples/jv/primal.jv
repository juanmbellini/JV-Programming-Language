    private static boolean isPrime(long number) {
        if (number == 2) {
            return true;
        }
        if (number < 2 || number % 2 == 0) {
            return false;
        }
        for (int i = 3; i < intSqrt(number); i+=2) {
            if (number % i == 0) {
                return false;
            }
        }
        return true;
    }


